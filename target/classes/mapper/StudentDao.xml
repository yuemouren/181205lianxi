<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.offcn.dao.StudentDao">
    <select id="getAllStu" resultType="com.offcn.bean.Student">
        SELECT * FROM newstudentinfo
    </select>
    
    <insert id="saveStu" parameterType="com.offcn.bean.Student">
    	insert into newstudentinfo (name,score,phone) values(#{name},#{score},#{phone})
    </insert>
</mapper>